      "source": [
        "## 95% Confidence Interval for Toyota Cars\n",
        "\n"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "cokgdUQblFCt",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "def confidence_interval(data, confidence_level=.95):\n",
        "  '''Takes in a pandas Series as input'''\n",
        "  xbar = data.mean()\n",
        "  n = len(data)\n",
        "  s = data.std(ddof=1)\n",
        "  t = stats.t.ppf((1 + confidence_level)/2,n-1)\n",
        "  standard_error = (s / np.sqrt(n))\n",
        "  margin_of_error = t*standard_error\n",
        "  lower_bound = xbar - margin_of_error\n",
        "  upper_bound = xbar + margin_of_error\n",
        "\n",
        "  return (lower_bound, xbar, upper_bound)\n",
        "\n",
        "  print(\" 95% Confidence Interval t statistic (with 6 degrees of freedom)\", t)"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "KEMStWP0mw0q",
