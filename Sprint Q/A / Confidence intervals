        "outputId": "4ec564c1-0f5e-494d-ccfc-2ff6fcdf9040",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 34
        }
      },
      "source": [
        "CI = confidence_interval(toyota['highway-mpg'])\n",
        "CI"
